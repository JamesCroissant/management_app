<%= render "shared/header" %>

<p id="notice"><%= notice %></p>

<h1>Schedules</h1>

<%= link_to 'イベント追加'%>

<div>
<%= month_calendar events: @schedules do |date, schedules| %>
  <%= date.day %>

  <div>
  <% schedules.each do |schedule| %>
    <div>
      <%= schedule.title %>
      <%= schedule.start_time %>
    </div>
  <% end %>
  </div>

<% end %>
</div>




